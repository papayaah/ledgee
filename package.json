{
  "name": "shawai",
  "version": "1.0.0",
  "description": "Shaw AI - Offline-first invoice extraction webapp using Chrome's built-in AI",
  "scripts": {
    "dev": "next dev --turbo -p 3002",
    "dev:legacy": "next dev",
    "build": "next build",
    "export": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next && rm -rf node_modules/.cache && rm -rf out",
    "dev:clean": "npm run clean && npm run dev",
    "build:static": "next build",
    "deploy": "npm run build:static && echo 'Static files ready in ./out/ - Deploy this folder!'",
    "serve:static": "npx serve out -p 8080"
  },
  "keywords": [
    "invoice",
    "ai",
    "extraction",
    "offline",
    "chrome-ai"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0"
  }
}
